# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{php,css,js,twig,json,xml}]
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true

[{Jenkinsfile,README.md}]
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true

[**/_market/**]
end_of_line = unset
indent_style = unset
indent_size = unset
insert_final_newline = unset

[/vendor/**]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset